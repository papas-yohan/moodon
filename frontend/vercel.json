{
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://backend-production-c41fe.up.railway.app/api/v1/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://backend-production-c41fe.up.railway.app/uploads/:path*"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "https://backend-production-c41fe.up.railway.app"
    },
    {
      "src": "/uploads/.*",
      "dest": "https://backend-production-c41fe.up.railway.app"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
